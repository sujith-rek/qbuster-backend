// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Reciept{
  orderId Int @id 
  orderDate DateTime
  orderTotal Float
  orderStatus Boolean
  shopId Int
}

model Shop{
  id Int @id
  name String
  address String
}